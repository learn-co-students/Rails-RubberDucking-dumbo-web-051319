<% if flash[:errors]  %>
<% flash[:errors].each do |error| %>
<li>
  <%= error %>
</li>
<% end %>
<% end %>

<%=  form_for @duck do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :student_id %>
<%= f.collection_select :student_id, @students, :id, :name %>

<%= f.submit "CREATE DIS" %>
<% end %>
