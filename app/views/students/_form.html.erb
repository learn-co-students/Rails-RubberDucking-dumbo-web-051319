<% if flash[:errors] %>
<ul>
<% flash[:errors].each do |error| %>
<li><%= error %></li>
<% end %>
</ul>
<% end %>

<h3>Student Info!!!</h3>
<%= form_for @student do |f|%>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :mod %>
<%= f.number_field :mod %>
<%= f.submit %>
<% end %><br>

<%=link_to "All Students", students_path %>
