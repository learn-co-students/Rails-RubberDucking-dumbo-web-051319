<h1>Edit your ducks here! </h1>
<%= form_for @duck do |f| %>
<label for="name">Name of Duck:</label>
  <%= f.text_field :name %>
  <label for="description">Description of Duck:</label>
  <%= f.text_field :description %>
  <label for="student_id">New student:</label>
  <%= f.collection_select :student_id, Student.all, :id, :name%>
<%= f.submit %>
<% end %>
