<h1>Edit Page</h1>
<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error_message| %>
      <li>
        <%= error_message %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= form_for @duck do |f| %>
  <%= f.label :name %>
  <%= f.collection_select :name, @ducks, :name, :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>
